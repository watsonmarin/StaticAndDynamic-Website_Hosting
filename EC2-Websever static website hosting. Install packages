#!/bin/bash
yum update -y
yum install httpd -y
chkconfig httpd on
service httpd start
cd /var/www/html
echo "<html><h1>Hello JJTECHInc Friends! Here is my Oregon, us-west-2 web server. My name is Mbandi Anyere my goal this year and the years to come is to have a steady income of 500K a year and 40K a month</h1></html>" > index.html
